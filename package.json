    "axios": "^0.21.4",
    "bootstrap": "^4.6",
    "caniuse-lite": "^1.0.30001363",
    "copyfiles": "^2.2.0",
    "moment": "^2.29.4",
    "next": "^11.1.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-moment": "^1.1.1",
    "reactstrap": "^8.9.0"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "globby": "^12.0.2"
